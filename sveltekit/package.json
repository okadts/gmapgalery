		"tailwindcss": "^3.2.4",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0"
	},
	"type": "module",
	"dependencies": {
		"@googlemaps/js-api-loader": "^1.15.1",
		"@tweenjs/tween.js": "^18.6.4",
		"nanoid": "^4.0.0"
